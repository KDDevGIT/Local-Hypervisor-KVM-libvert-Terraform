# Note: terraform.tfvars is part of .gitignore and will not push 

vm_count            = 3
vm_name_prefix      = "lab"
memory_mb           = 2048
vcpu                = 2
disk_size_gb        = 20
network_name        = "default"
ssh_public_key_file = "~/.ssh/id_ed25519.pub"
